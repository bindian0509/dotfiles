UXTerm*color15:    rgb:ff/ff/ff

UXTerm*dynamicColors:      true

! Fonts
UXTerm*facename:           terminus:bold:pixelsize=14
UXTerm*font:               -*-terminus-medium-r-*-*-12-*-*-*-*-*-*-*

! Cursor
UXTerm*cursorBlink:        true

! Copypasta
UXTerm*cutNewline:         false
UXTerm*on4Clicks:          group
UXTerm*on5Clicks:          page

! Input
UXTerm*eightBitInput:      true

! Scroll
UXTerm*saveLines:          8192
UXTerm*scrollKey:          true
UXTerm*scrollTtyOutput:    false

! UTF-8
UXTerm*utf8:               2
UXTerm*showMissingGlyphs:  true
